{
    "batch_size": 1,
    "pipeline": [
        {
            "name": "object",
            "path": "cfg/object.json",
            "input_map": [
                {
                    "key": "input",
                    "val": "entry"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "frame"
                }
            ]
        },
        {
            "name": "decode",
            "path": "./plugins/official/libcpuyuvdec.so",
            "input_map": [
                {
                    "key": "decode_input",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "decode_output",
                    "val": "yuv"
                }
            ]
        },
        {
            "name": "detection",
            "path": "./plugins/libfacedetection/libdetection2.so",
            "input_map": [
                {
                    "key": "detection_input",
                    "val": "yuv"
                }
            ],
            "output_map": [
                {
                    "key": "detection_output",
                    "val": "meta_yuv"
                }
            ],
            "params": {
              "model": "./data/model/libfacedetection/yunet_120x160.onnx",
              "backend_id": 0,
              "target_id": 0,
              "score_threshold": 0.9,
              "nms_threshold": 0.3,
              "top_k": 5000,
              "skip": 1
            }
        },
        {
            "name": "osd",
            "path": "./plugins/official/libosd.so",
            "input_map": [
                {
                    "key": "osd_input",
                    "val": "meta_yuv"
                }
            ],
            "output_map": [
                {
                    "key": "osd_output",
                    "val": "osd_264"
                }
            ],
            "params": {
              "color": [0, 255, 0],
              "thickness": 1,
              "bitrate": 2000000,
              "gop": 100
            }
        },
        {
            "name": "preview",
            "path": "./plugins/official/libpreview.so",
            "input_map": [
                {
                    "key": "preview_input",
                    "val": "osd_264"
                }
            ],
            "output_map": [
                {
                    "key": "preview_output",
                    "val": "null"
                }
            ]
        }
    ]
}

