{
    "batch_size": 1,
    "pipeline": [
        {
            "name": "object",
            "path": "cfg/object.json",
            "input_map": [
                {
                    "key": "input",
                    "val": "entry"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "frame"
                }
            ]
        },
        {
            "name": "decode",
            "path": "./lib/libdec.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "yuv"
                }
            ]
        },
        {
            "name": "yuv2rgb",
            "path": "./lib/libyuv2rgb.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "yuv"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "rgb"
                }
            ],
            "params": {
                "layout": "NHWC",
                "format": "BGR",
                "resize": [416,416],
                "normalation": "common"
            }
            
        },
        {
            "name": "preprocess",
            "path": "./lib/libpreprocess.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "rgb"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "preprocess_result"
                }
            ]
        },
        {
            "name": "yolov3",
            "path": "./lib/tvm_yolov3.so",
            "framework": "tvm",
            "input_map": [
                {
                    "key": "input",
                    "val": "preprocess_result"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "yololayer3"
                }
            ]
        },
        {
            "name": "postprocess",
            "path": "./lib/libpostprocess.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "yololayer3"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "postprocess_result"
                }
            ]
        },
        {
            "name": "tracker",
            "path": "./lib/libtracker.so",
            "async": "true",
            "input_map": [
                {
                    "key": "input",
                    "val": "postprocess_result"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "tracker_result"
                }
            ]
        },
        {
            "name": "rabbitmq",
            "path": "./lib/librabbitmq.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "tracker_result"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "null"
                }
            ],
            "params": {
                "host": "10.0.0.10",
                "port": 5672,
                "username": "guest",
                "password": "guest",
                "exchange": "amq.direct",
                "routingkey": ""
            }
        },
        {
            "name": "preview",
            "path": "./plugins/official/libpreview.so",
            "input_map": [
                {
                    "key": "input",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "null"
                }
            ],
            "params": {
                "type": "hls"
            }
        },
        {
            "name": "display",
            "path": "./lib/libdisplay.so",
            "input_map": [
                {
                    "key": "input1",
                    "val": "rgb"
                },
                {
                    "key": "input2",
                    "val": "tracker_result"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "null"
                }
            ]
        }
    ]
}

