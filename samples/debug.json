{
    "batch_size": 1,
    "pipeline": [
        {
            "name": "object",
            "path": "cfg/object.json",
            "input_map": [
                {
                    "key": "input",
                    "val": "entry"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "frame"
                }
            ]
        },
        {
            "name": "debug",
            "path": "./plugins/debug/libdebug.so",
            "input_map": [
                {
                    "key": "debug_input",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "debug_output",
                    "val": "meta_yuv"
                }
            ]
        },
        {
            "name": "osd",
            "path": "./plugins/official/libosd.so",
            "input_map": [
                {
                    "key": "osd_input",
                    "val": "meta_yuv"
                }
            ],
            "output_map": [
                {
                    "key": "osd_output",
                    "val": "osd_264"
                }
            ],
            "params": {
              "color": [255, 0, 0],
              "thickness": 1,
              "bitrate": 2000000,
              "gop": 100
            }
        },
        {
            "name": "preview",
            "path": "./plugins/official/libpreview.so",
            "input_map": [
                {
                    "key": "preview_input",
                    "val": "osd_264"
                }
            ],
            "output_map": [
                {
                    "key": "preview_output",
                    "val": "null"
                }
            ]
        }
    ]
}

