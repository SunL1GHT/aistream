{
    "batch_size": 1,
    "pipeline": [
        {
            "name": "object",
            "path": "cfg/object.json",
            "input_map": [
                {
                    "key": "input",
                    "val": "entry"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "frame"
                }
            ]
        },
        {
            "name": "debug",
            "path": "./plugins/debug/libdebug.so",
            "input_map": [
                {
                    "key": "debug_input_frame",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "osd"
                }
            ]
        },
        {
            "name": "preview",
            "path": "./plugins/official/libpreview.so",
            "async": "true",
            "input_map": [
                {
                    "key": "preview_input1_frame",
                    "val": "frame"
                },
                {
                    "key": "preview_input2_osd",
                    "val": "osd"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "null"
                }
            ],
            "params": {
                "type": "hls"
            }
        }
    ]
}

