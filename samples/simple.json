{
    "batch_size": 1,
    "pipeline": [
        {
            "name": "object",
            "path": "cfg/object.json",
            "input_map": [
                {
                    "key": "input",
                    "val": "entry"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "frame"
                }
            ]
        },
        {
            "name": "preview",
            "path": "./plugins/lib/libpreview.so",
            "async": "true",
            "input_map": [
                {
                    "key": "input",
                    "val": "frame"
                }
            ],
            "output_map": [
                {
                    "key": "output",
                    "val": "null",
                    "params": {
                        "type": "hls"
                    }
                }
            ]
        }
    ]
}

